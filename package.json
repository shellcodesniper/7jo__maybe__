{
  "name": "Capstone",
  "version": "0.0.1",
  "description": "Capstone 7jo",
  "main": "bin/www.js",
  "author": "shellcodesniper",
  "license": "MIT",
  "scripts": {
    "test": "TS_NODE_PROJECT='./tsconfig.json' mocha -r node_modules/ts-node/register -r tsconfig-paths/register ./bin/*.spec.ts ./src/**/*.spec.ts",
    "clean": "rm -rf ./dist",
    "checkOnly": "yarn lint && tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "prestart": "yarn clean; yarn build",
    "start": "NODE_ENV=production nodemon -r ./tsconfig-paths-bootstrap.js dist/bin/www.js",
    "prebuild": "yarn checkOnly && yarn clean; echo '정상.'",
    "build": "tsc --project ./tsconfig.build.json",
    "postbuild": "cp -rf ./public ./dist/ && echo '완료'",
    "dev": "NODE_PATH=src NODE_ENV=development nodemon -r tsconfig-paths/register --watch src --delay 1 --exec 'ts-node' ./bin/www.ts",
    "prod": "NODE_PATH=src NODE_ENV=production nodemon -r tsconfig-paths/register --watch src --delay 1 --exec 'ts-node' ./bin/www.ts",
    "debug": "NODE_PATH=src NODE_ENV=development DEBUG=* nodemon -r tsconfig-paths/register --watch src --delay 1 --exec 'ts-node' ./bin/www.ts",
    "docker:start": "NODE_ENV=production pm2-docker start ./dist/bin/www.js --node-args=\"-r ./tsconfig-paths-bootstrap.js\" -i max",
    "dangerDeploy": "yarn build && sh ./bin/dangerDeploy.sh $npm_package_version",
    "safeDeploy": "yarn build && sh ./bin/safeDeploy.sh $npm_package_version",
    "danger-docker-major": "npm version major --no-git-tag-version;yarn dangerDeploy",
    "danger-docker-minor": "npm version minor --no-git-tag-version;yarn dangerDeploy",
    "danger-docker-patch": "npm version patch --no-git-tag-version;yarn dangerDeploy",
    "safe-docker-major": "npm version major --no-git-tag-version;yarn safeDeploy",
    "safe-docker-minor": "npm version minor --no-git-tag-version;yarn safeDeploy",
    "safe-docker-patch": "npm version patch --no-git-tag-version;yarn safeDeploy",
    "deploy-patch": "clear;printf \"\n\n\n\";npm version patch --no-git-tag-version;printf \"\n\n\n 푸시하시고, 머지요청하셔야 패치됩니다.\n\n\n\n\n\n\n\n\n\n\n\n\n\";printf \"https://github.com/shellcodesniper/MultiTool_API/compare\n\n\";git add package.json && git commit && git push",
    "deploy-minor": "clear;printf \"\n\n\n\";npm version minor --no-git-tag-version;printf \"\n\n\n 푸시하시고, 머지요청하셔야 패치됩니다.\n\n\n\n\n\n\n\n\n\n\n\n\n\";printf \"https://github.com/shellcodesniper/MultiTool_API/compare\n\n\";git add package.json && git commit && git push",
    "deploy-major": "clear;printf \"\n\n\n\";npm version major --no-git-tag-version;printf \"\n\n\n 푸시하시고, 머지요청하셔야 패치됩니다.\n\n\n\n\n\n\n\n\n\n\n\n\n\";printf \"https://github.com/shellcodesniper/MultiTool_API/compare\n\n\";git add package.json && git commit && git push"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn checkOnly"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.888.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "form-data": "^4.0.0",
    "formidable": "^1.2.2",
    "got": "^11.8.2",
    "iconv-lite": "^0.6.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compress": "^5.0.1",
    "koa-connect": "^2.1.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "mongoose": "^5.12.4",
    "pbkdf2": "^3.1.2",
    "pm2": "^4.5.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/formidable": "^1.0.32",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.7",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/mocha": "^8.2.0",
    "@types/mongoose": "^5.10.3",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/node": "^14.14.22",
    "@types/pbkdf2": "^3.1.0",
    "@types/rsmq-worker": "^0.3.29",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.2.5",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.1.1"
  }
}
